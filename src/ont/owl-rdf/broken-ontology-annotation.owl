<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.example.com/iri#"
     xml:base="http://www.example.com/iri"
     xmlns:o="http://www.example.com/iri#"
     xmlns:iri="http://www.example.com/iri/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.example.com/iri">
        <owl:versionIRI rdf:resource="http://www.example.com/viri"/>
        <iri:propformat-version xml:lang="en">1.2</iri:propformat-version>
    </owl:Ontology>

    <!--
        Some versions of the OWL API do not add AnnotationProperty
        declarations for ontology annotations. I think this is broken
        but enough ontologies use it that we should probably check for it.

    --> 

</rdf:RDF>



